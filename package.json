{
  "name": "metacoin-source-verify",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "ipfs": "node --no-warnings ./scripts/ipfs.js",
    "deploy:ropsten": "truffle compile --all && npm run ipfs && truffle migrate --f 1 --to 1 --network ropsten",
    "deploy:rinkeby": "truffle compile --all && npm run ipfs && truffle migrate --f 1 --to 1 --network rinkeby",
    "deploy:goerli": "truffle compile --all && npm run ipfs && truffle migrate --f 1 --to 1 --network goerli",
    "deploy-with-salt:goerli": "./salt.sh && truffle compile --all && truffle migrate --f 2 --to 2 --network goerli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cgewecke/metacoin-source-verify.git"
  },
